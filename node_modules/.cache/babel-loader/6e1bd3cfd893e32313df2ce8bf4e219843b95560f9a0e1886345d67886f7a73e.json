0435\\u043A\\u0442\\u043E\\u0440\\u043E\\u043C \\u0443\\u0447\\u0440\\u0435\\u0436\\u0434\\u0435\\u043D\\u0438\\u044F \\u0438 \\u043C\\u043E\\u0436\\u0435\\u0442 \\u043F\\u0440\\u043E\\u0445\\u043E\\u0434\\u0438\\u0442\\u044C \\u0432\\u043D\\u0435\\u0448\\u043D\\u044E\\u044E \\u044D\\u043A\\u0441\\u043F\\u0435\\u0440\\u0442\\u0438\\u0437\\u0443 (\\u043D\\u0430\\u043F\\u0440\\u0438\\u043C\\u0435\\u0440, \\u0430\\u043A\\u043A\\u0440\\u0435\\u0434\\u0438\\u0442\\u0430\\u0446\\u0438\\u044E \\u0438\\u043B\\u0438 \\u043E\\u0431\\u0449\\u0435\\u0441\\u0442\\u0432\\u0435\\u043D\\u043D\\u043E\\u0435 \\u043E\\u0431\\u0441\\u0443\\u0436\\u0434\\u0435\\u043D\\u0438\\u0435).\"]})]}),/*#__PURE__*/_jsxs(ContactForm,{onSubmit:handleSubmit,children:[/*#__PURE__*/_jsx(Label,{htmlFor:\"fio\",children:\"\\u0415\\u0441\\u043B\\u0438 \\u0432\\u044B \\u0443\\u0436\\u0435 \\u0441\\u0434\\u0435\\u043B\\u0430\\u043B\\u0438 \\u0438\\u043B\\u0438 \\u0443 \\u0432\\u0430\\u0441 \\u0435\\u0441\\u0442\\u044C \\u043F\\u0440\\u0435\\u0434\\u043B\\u043E\\u0436\\u0435\\u043D\\u0438\\u044F \\u0438\\u0437\\u043C\\u0435\\u043D\\u0435\\u043D\\u0438\\u044F \\u043D\\u044B\\u043D\\u0435\\u0448\\u043D\\u0435\\u0439 \\u043E\\u0431\\u0440\\u0430\\u0437\\u043E\\u0432\\u0430\\u0442\\u0435\\u043B\\u044C\\u043D\\u043E \\u043F\\u0440\\u043E\\u0433\\u0440\\u0430\\u043C\\u043C\\u044B \\u043C\\u043E\\u0436\\u0438\\u0442\\u0435 \\u0437\\u0430\\u043F\\u043E\\u043B\\u043D\\u0438\\u0442\\u044C \\u0438 \\u043E\\u0442\\u043F\\u0440\\u0430\\u0432\\u0438\\u0442\\u044C \\u044D\\u0442\\u0443 \\u0444\\u043E\\u0440\\u043C\\u0443\"}),/*#__PURE__*/_jsxs(FormGroup,{children:[/*#__PURE__*/_jsx(Label,{htmlFor:\"fio\",children:\"\\u0424\\u0418\\u041E:\"}),/*#__PURE__*/_jsx(Input,{type:\"text\",id:\"fio\",name:\"fio\",value:formData.fio,onChange:handleChange,required:true})]}),/*#__PURE__*/_jsxs(FormGroup,{children:[/*#__PURE__*/_jsx(Label,{htmlFor:\"organization\",children:\"\\u041D\\u0430\\u0437\\u0432\\u0430\\u043D\\u0438\\u0435 \\u043E\\u0440\\u0433\\u0430\\u043D\\u0438\\u0437\\u0430\\u0446\\u0438\\u0438:\"}),/*#__PURE__*/_jsx(Input,{type:\"text\",id:\"organization\",name:\"organization\",value:formData.organization,onChange:handleChange,required:true})]}),/*#__PURE__*/_jsxs(FormGroup,{children:[/*#__PURE__*/_jsx(Label,{htmlFor:\"email\",children:\"\\u0412\\u0430\\u0448 email:\"}),/*#__PURE__*/_jsx(Input,{type:\"email\",id:\"email\",name:\"email\",value:formData.email,onChange:handleChange,rows:\"4\",required:true})]}),/*#__PURE__*/_jsxs(FormGroup,{children:[/*#__PURE__*/_jsx(Label,{htmlFor:\"message\",children:\"\\u0421\\u043E\\u043E\\u0431\\u0449\\u0435\\u043D\\u0438\\u0435:\"}),/*#__PURE__*/_jsx(TextArea,{id:\"message\",name:\"message\",value:formData.message,onChange:handleChange,rows:\"4\",required:true})]}),/*#__PURE__*/_jsxs(FormGroup,{children:[/*#__PURE__*/_jsx(Label,{htmlFor:\"file\",children:\"\\u041F\\u0440\\u0438\\u043A\\u0440\\u0435\\u043F\\u0438\\u0442\\u044C \\u0444\\u0430\\u0439\\u043B \\u0441 \\u043E\\u0431\\u0440\\u0430\\u0437\\u043E\\u0432\\u0430\\u0442\\u0435\\u043B\\u044C\\u043D\\u043E\\u0439 \\u043F\\u0440\\u043E\\u0433\\u0440\\u0430\\u043C\\u043C\\u043E\\u0439 (JSON):\"}),/*#__PURE__*/_jsx(FileInput,{type:\"file\",id:\"file\",name:\"file\",accept:\".json\",onChange:handleChange})]}),/*#__PURE__*/_jsx(SubmitButton,{type:\"submit\",children:\"\\u041E\\u0442\\u043F\\u0440\\u0430\\u0432\\u0438\\u0442\\u044C\"})]}),/*#__PURE__*/_jsxs(CreatorInfo,{children:[\"\\u0420\\u0430\\u0437\\u0440\\u0430\\u0431\\u043E\\u0442\\u0430\\u043D\\u043E \\u0441\\u0442\\u0443\\u0434\\u0435\\u043D\\u0442\\u043E\\u043C \\u0411\\u0424\\u0423 \\u043D\\u0430\\u043F\\u0440\\u0430\\u0432\\u043B\\u0435\\u043D\\u0438\\u044F \\\"\\u0418\\u043D\\u0444\\u043E\\u0440\\u043C\\u0430\\u0446\\u0438\\u043E\\u043D\\u043D\\u0430\\u044F \\u0431\\u0435\\u0437\\u043E\\u043F\\u0430\\u0441\\u043D\\u043E\\u0441\\u0442\\u044C\\\" : \\u0417\\u0443\\u043B\\u044C\\u0444\\u0438\\u043A\\u0430\\u0440\\u043E\\u0432\\u044B\\u043C \\u0414\\u0430\\u043D\\u0438\\u043B\\u043E\\u043C \\u041C\\u0443\\u0440\\u0430\\u0442\\u043E\\u0432\\u0438\\u0447\\u0435\\u043C \",/*#__PURE__*/_jsx(\"br\",{}),\"Email: obrazovatelnaiprogramm@gmail.com\",/*#__PURE__*/_jsx(\"br\",{}),\"GitHub: \",/*#__PURE__*/_jsx(\"a\",{href:\"https://github.com/zukdan\",target:\"_blank\",rel:\"noopener noreferrer\",children:\"zukdan\"}),/*#__PURE__*/_jsx(\"br\",{})]})]});}export default LandingPage;","map":{"version":3,"names":["React","useState","Link","styled","axios","jsx","_jsx","jsxs","_jsxs","LandingPageContainer","div","Title","h1","Description","p","AppButton","a","InfoSection","InfoTitle","h2","InfoParagraph","ContactForm","form","FormGroup","Label","label","Input","input","TextArea","textarea","FileInput","SubmitButton","button","CreatorInfo","LandingPage","formData","setFormData","fio","organization","email","message","file","handleChange","e","name","value","type","files","target","prevData","handleSubmit","preventDefault","formDataToSend","FormData","append","response","post","headers","console","log","data","alert","error","children","as","href","rel","onSubmit","htmlFor","id","onChange","required","rows","accept"],"sources":["C:/Users/zulda/Desktop/5/src/components/LandingPage.js"],"sourcesContent":["import React, { useState } from 'react';\r\nimport { Link } from 'react-router-dom';\r\nimport styled from 'styled-components';\r\nimport axios from 'axios';\r\n\r\nconst LandingPageContainer = styled.div`\r\n  text-align: center;\r\n  padding: 20px;\r\n`;\r\n\r\nconst Title = styled.h1`\r\n  font-size: 2.5em;\r\n  margin-bottom: 20px;\r\n`;\r\n\r\nconst Description = styled.p`\r\n  font-size: 1.2em;\r\n  margin-bottom: 30px;\r\n`;\r\n\r\nconst AppButton = styled.a`\r\n  background-color: #4CAF50;\r\n  border: none;\r\n  color: white;\r\n  padding: 15px 32px;\r\n  text-align: center;\r\n  text-decoration: none;\r\n  display: inline-block;\r\n  font-size: 1.2em;\r\n  margin: 4px 2px;\r\n  cursor: pointer;\r\n  border-radius: 5px;\r\n\r\n  &:hover {\r\n    background-color: #367C39;\r\n  }\r\n`;\r\n\r\nconst InfoSection = styled.div`\r\n  text-align: left;\r\n  margin-top: 40px;\r\n  padding: 20px;\r\n  border: 1px solid #ddd;\r\n  border-radius: 5px;\r\n`;\r\n\r\nconst InfoTitle = styled.h2`\r\n  font-size: 1.5em;\r\n  margin-bottom: 15px;\r\n`;\r\n\r\nconst InfoParagraph = styled.p`\r\n  font-size: 1.1em;\r\n  line-height: 1.6;\r\n`;\r\n\r\nconst ContactForm = styled.form`\r\n  text-align: left;\r\n  margin-top: 40px;\r\n  padding: 20px;\r\n  border: 1px solid #ddd;\r\n  border-radius: 5px;\r\n`;\r\n\r\nconst FormGroup = styled.div`\r\n  margin-bottom: 15px;\r\n`;\r\n\r\nconst Label = styled.label`\r\n  display: block;\r\n  font-weight: bold;\r\n  margin-bottom: 5px;\r\n`;\r\n\r\nconst Input = styled.input`\r\n  width: 100%;\r\n  padding: 8px;\r\n  border: 1px solid #ccc;\r\n  border-radius: 4px;\r\n  box-sizing: border-box; /* Important! */\r\n`;\r\n\r\nconst TextArea = styled.textarea`\r\n  width: 100%;\r\n  padding: 8px;\r\n  border: 1px solid #ccc;\r\n  border-radius: 4px;\r\n  box-sizing: border-box; /* Important! */\r\n  resize: vertical;\r\n`;\r\n\r\nconst FileInput = styled.input`\r\n  padding: 8px 0;\r\n`;\r\n\r\nconst SubmitButton = styled.button`\r\n  background-color: #007bff;\r\n  color: white;\r\n  padding: 10px 20px;\r\n  border: none;\r\n  border-radius: 4px;\r\n  cursor: pointer;\r\n\r\n  &:hover {\r\n    background-color: #0056b3;\r\n  }\r\n`;\r\n\r\nconst CreatorInfo = styled.div`\r\n  margin-top: 50px;\r\n  font-size: 0.9em;\r\n  color: #777;\r\n`;\r\n\r\nfunction LandingPage() {\r\n  const [formData, setFormData] = useState({\r\n      fio: '',\r\n      organization: '',\r\n      email: '',\r\n      message: '',\r\n      file: null,\r\n  });\r\n\r\n  const handleChange = (e) => {\r\n      const { name, value, type, files } = e.target;\r\n      setFormData(prevData => ({\r\n          ...prevData,\r\n          [name]: type === 'file' ? files[0] : value,\r\n      }));\r\n  };\r\n\r\n  const handleSubmit = async (e) => {\r\n      e.preventDefault();\r\n\r\n      const formDataToSend = new FormData();\r\n      formDataToSend.append('fio', formData.fio);\r\n      formDataToSend.append('organization', formData.organization);\r\n      formDataToSend.append('email', formData.email);\r\n      formDataToSend.append('message', formData.message);\r\n      if (formData.file) {\r\n          formDataToSend.append('file', formData.file, formData.file.name);\r\n      }\r\n\r\n      try {\r\n          const response = await axios.post('http://localhost:5000/send-email', formDataToSend, {\r\n              headers: {\r\n                  'Content-Type': 'multipart/form-data'\r\n              }\r\n          });\r\n\r\n          console.log(response.data);\r\n          alert('Сообщение успешно отправлено!');\r\n\r\n          setFormData({\r\n              fio: '',\r\n              organization: '',\r\n              email: '',\r\n              message: '',\r\n              file: null,\r\n          });\r\n      } catch (error) {\r\n          console.error('Ошибка при отправке:', error);\r\n          alert('Ошибка при отправке сообщения.');\r\n      }\r\n  };\r\n\r\n  return (\r\n    <LandingPageContainer>\r\n      <Title>Добро пожаловать 👋</Title>\r\n      <Description>\r\n        Это сайт создан для помощи в визуализации и формировании образовательной программы по направлению \"Информационная безопастность\"\r\n      </Description>\r\n\r\n      <AppButton\r\n          as=\"a\"\r\n          href=\"/app\" \r\n          target=\"_blank\"\r\n          rel=\"noopener noreferrer\"\r\n      >\r\n        Перейти к веб приложению для визуализации и формирования образовательной программы по направлению \"Информационная безопастность\"\r\n      </AppButton>\r\n      \r\n      <InfoSection>\r\n        <InfoTitle>Обязательные требования для создания образовательной программы:</InfoTitle>\r\n        <InfoParagraph>\r\n          Не стоит забывать , что образовательная программа разрабатывается с учетом следующих аспектов:\r\n        </InfoParagraph>\r\n        <InfoParagraph>\r\n          <b>1. Соответствие стандартам:</b> Программа основывается на ФГОС (Федеральный государственный образовательный стандарт), определяющем требования к знаниям и умениям выпускника, профстандартах (для профессионального образования) и локальных актах учебного заведения.\r\n        </InfoParagraph>\r\n        <InfoParagraph>\r\n          <b>2. Профиль подготовки и цели:</b> Учитывается профиль подготовки (например, педагог, инженер, врач), определяются основные цели обучения, включая компетенции, знания и навыки, которые должен получить студент.\r\n        </InfoParagraph>\r\n        <InfoParagraph>\r\n          <b>3. Учебный план:</b> Разрабатывается учебный план, включающий перечень дисциплин, модулей и их объем в часах/кредитах, а также график учебного процесса с последовательностью изучения предметов, каникулами и практикой. Программы дисциплин (рабочие программы) содержат подробное содержание каждого предмета и формы контроля.\r\n        </InfoParagraph>\r\n        <InfoParagraph>\r\n          <b>4. Воспитательная работа:</b> Программа включает раздел воспитательной работы, отражающий мероприятия по гражданско-патриотическому, духовно-нравственному, экологическому воспитанию, волонтёрскую и проектную деятельность, психолого-педагогическое сопровождение студентов и сотрудничество с родителями и социальными партнёрами.\r\n        </InfoParagraph>\r\n        <InfoParagraph>\r\n          <b>5. Апробация и утверждение:</b> Программа обсуждается на методических советах, утверждается директором или ректором учреждения и может проходить внешнюю экспертизу (например, аккредитацию или общественное обсуждение).\r\n        </InfoParagraph>\r\n\r\n      </InfoSection>\r\n\r\n      <ContactForm onSubmit={handleSubmit}>\r\n                <Label htmlFor=\"fio\">Если вы уже сделали или у вас есть предложения изменения нынешней образовательно программы можите заполнить и отправить эту форму</Label>\r\n                <FormGroup>\r\n                    <Label htmlFor=\"fio\">ФИО:</Label>\r\n                    <Input\r\n                        type=\"text\"\r\n                        id=\"fio\"\r\n                        name=\"fio\"\r\n                        value={formData.fio}\r\n                        onChange={handleChange}\r\n                        required\r\n                    />\r\n                </FormGroup>\r\n                <FormGroup>\r\n                    <Label htmlFor=\"organization\">Название организации:</Label>\r\n                    <Input\r\n                        type=\"text\"\r\n                        id=\"organization\"\r\n                        name=\"organization\"\r\n                        value={formData.organization}\r\n                        onChange={handleChange}\r\n                        required\r\n                    />\r\n                </FormGroup>\r\n                <FormGroup>\r\n                    <Label htmlFor=\"email\">Ваш email:</Label>\r\n                    <Input\r\n                        type=\"email\"\r\n                        id=\"email\"\r\n                        name=\"email\"\r\n                        value={formData.email}\r\n                        onChange={handleChange}\r\n                        rows=\"4\"\r\n                        required \r\n                        />\r\n                </FormGroup>\r\n                <FormGroup>\r\n          <Label htmlFor=\"message\">Сообщение:</Label>\r\n          <TextArea\r\n            id=\"message\"\r\n            name=\"message\"\r\n            value={formData.message}\r\n            onChange={handleChange}\r\n            rows=\"4\"\r\n            required\r\n          />\r\n        </FormGroup> \r\n                <FormGroup>\r\n                    <Label htmlFor=\"file\">Прикрепить файл с образовательной программой (JSON):</Label>\r\n                    <FileInput\r\n                        type=\"file\"\r\n                        id=\"file\"\r\n                        name=\"file\"\r\n                        accept=\".json\"\r\n                        onChange={handleChange}\r\n                    />\r\n                </FormGroup>\r\n                <SubmitButton type=\"submit\">Отправить</SubmitButton>\r\n            </ContactForm>\r\n\r\n            <CreatorInfo>\r\n                Разработано студентом БФУ направления \"Информационная безопасность\" : Зульфикаровым Данилом Муратовичем <br />\r\n                Email: obrazovatelnaiprogramm@gmail.com<br />\r\n                GitHub: <a href=\"https://github.com/zukdan\" target=\"_blank\" rel=\"noopener noreferrer\">zukdan</a><br />\r\n            </CreatorInfo>\r\n        </LandingPageContainer>\r\n    );\r\n}\r\n\r\nexport default LandingPage;"],"mappings":"AAAA,MAAO,CAAAA,KAAK,EAAIC,QAAQ,KAAQ,OAAO,CACvC,OAASC,IAAI,KAAQ,kBAAkB,CACvC,MAAO,CAAAC,MAAM,KAAM,mBAAmB,CACtC,MAAO,CAAAC,KAAK,KAAM,OAAO,CAAC,OAAAC,GAAA,IAAAC,IAAA,CAAAC,IAAA,IAAAC,KAAA,yBAE1B,KAAM,CAAAC,oBAAoB,CAAGN,MAAM,CAACO,GAAG;AACvC;AACA;AACA,CAAC,CAED,KAAM,CAAAC,KAAK,CAAGR,MAAM,CAACS,EAAE;AACvB;AACA;AACA,CAAC,CAED,KAAM,CAAAC,WAAW,CAAGV,MAAM,CAACW,CAAC;AAC5B;AACA;AACA,CAAC,CAED,KAAM,CAAAC,SAAS,CAAGZ,MAAM,CAACa,CAAC;AAC1B;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA,CAAC,CAED,KAAM,CAAAC,WAAW,CAAGd,MAAM,CAACO,GAAG;AAC9B;AACA;AACA;AACA;AACA;AACA,CAAC,CAED,KAAM,CAAAQ,SAAS,CAAGf,MAAM,CAACgB,EAAE;AAC3B;AACA;AACA,CAAC,CAED,KAAM,CAAAC,aAAa,CAAGjB,MAAM,CAACW,CAAC;AAC9B;AACA;AACA,CAAC,CAED,KAAM,CAAAO,WAAW,CAAGlB,MAAM,CAACmB,IAAI;AAC/B;AACA;AACA;AACA;AACA;AACA,CAAC,CAED,KAAM,CAAAC,SAAS,CAAGpB,MAAM,CAACO,GAAG;AAC5B;AACA,CAAC,CAED,KAAM,CAAAc,KAAK,CAAGrB,MAAM,CAACsB,KAAK;AAC1B;AACA;AACA;AACA,CAAC,CAED,KAAM,CAAAC,KAAK,CAAGvB,MAAM,CAACwB,KAAK;AAC1B;AACA;AACA;AACA;AACA;AACA,CAAC,CAED,KAAM,CAAAC,QAAQ,CAAGzB,MAAM,CAAC0B,QAAQ;AAChC;AACA;AACA;AACA;AACA;AACA;AACA,CAAC,CAED,KAAM,CAAAC,SAAS,CAAG3B,MAAM,CAACwB,KAAK;AAC9B;AACA,CAAC,CAE